<body>

  <nav class="navbar navbar-expand-lg  shadow-sm main ">
    <div class="container-fluid ">
      <button class="navbar-toggler" type="button" data-bs-toggle="offcanvas" data-bs-target="#offcanvasNavbar"
        aria-controls="offcanvasNavbar">
        <span class="navbar-toggler-icon"></span>
      </button>

      <a class="navbar-brand " id="logo" href="#"><img src="Images/Logo.avif" alt="" width="160" height="50"></a>
      <div class="offcanvas offcanvas-start " tabindex="-1" id="offcanvasNavbar" aria-labelledby="offcanvasNavbarLabel">
        <div class="offcanvas-header">

          <button type="button" class="btn-close" data-bs-dismiss="offcanvas" aria-label="Close"></button>
        </div>

        <div class="offcanvas-body">
          <ul class="navbar-nav justify-content-center ms-5 ">
            <li class="nav-item">
              <a class="nav-link mx-lg-1 fon" aria-current="page" href="#">HOME</a>
            </li>
            <li class="nav-item">
              <a class="nav-link mx-lg-1 fon" href="#face">FACE MAKEUP</a>
            </li>
            <li class="nav-item">
              <a class="nav-link mx-lg-1 fon" href="#eyes">EYE MAKEUP</a>
            </li>
            <li class="nav-item">
              <a class="nav-link mx-lg-1 fon" href="#lips">LIP MAKEUP</a>
            </li>
            <li class="nav-item">
              <a class="nav-link mx-lg-1 fon" href="#Accesories">ACCESSORIES</a>
            </li>
            <li class="nav-item">
              <a class="nav-link mx-lg-1 fon" href="ContactUs.html">CONTACT US</a>
            </li>
            <li class="nav-item">
              <a class="nav-link mx-lg-1 fon" href="FAQs.html">FAQs</a>
            </li>
          </ul>
        </div>
      </div>

      <div class="d-flex justify-content-center align-items-center gap-1 fs-5 " id="icons">
        <% if (!user) { %>
          <a class="link-dark " href="/login">
            <button class="btn"><i class="fa-solid fa-user"></i></button>
          </a>

          <% } else { %>
            <a class="link-dark " href="/logout">
              <button class="btn"><i class="fa-solid fa-sign-out-alt"></i></button>
            </a>
            <!-- <li>Welcome <%= user.name %>!</li> -->
            <% } %>


              <button id="cartButton" class="btn"><i class="fa-solid fa-cart-shopping"></i>
                <span id="cartBadge"
                  class="position-absloute top-0  translate-middle badge rounded-pill bg-danger">0</span>
              </button>
              <div id="overlay" class="overlay"></div>

              <div id="cart" class="cart">

                <span class="close"><img src="/Images/close.svg" alt=""></span>
                <div class="mt-5 fs-2 fw-medium  text-center">CART</div>
                <div class="mt-2 cart-height cart-content  overflow-auto">


                </div>

                <div class="d-flex justify-content-center ">
                  <div id="total" class="mt-4 fs-3  ">SubTotal: <span class="ps-5 " id="subtotal">$0</span> </div>
                </div>
                <div class="d-flex justify-content-center ">
                  <a class="text-decoration-none text-white " href="/Check/Cart">
                    <button id="CheckOut" class="btn-dark outbtn  pt-1 pb-1 ps-2 pe-2  m-3" disabled>Check Out</button>
                  </a>
                </div>
              </div>
      </div>

    </div>
    </div>
  </nav>


  <div class="background">
    <img src="Images/background.jpg" alt="">
  </div>

  <h4 id="face" class="display-5 contact  p-5 ">FACE MAKEUP</h4>

  <div class="container">
    <div class="row">
      <% faceproducts.forEach(product=> { %>
        <div class="col-xl-3 col-lg-3 col-md-6 col-sm-12">
          <a class="text-decoration-none" href="/Face/<%= product.id %>">
            <div class="card mb-4">
              <img src="<%= product.imageUrl %>" class="card-img-top" alt="<%= product.title %>">
              <div class="card-body">
                <h5 class="card-title">
                  <%= product.title %>
                </h5>
                <p class="card-text fs-3">$<%= product.price %>
                </p>
              </div>
            </div>
          </a>
        </div>
        <% }); %>
    </div>
  </div>

  <div class=" mt-4 d-flex justify-content-center">
    <button id="one" class="boton">View all</button>
  </div>


